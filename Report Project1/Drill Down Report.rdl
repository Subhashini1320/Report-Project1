  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
	  Cust.First_Name as CustomerName    
	  ,Year([order_date]) OrderedYear
	  ,Sum(item.Quantity) as Quantity
	  ,Sum((item.List_price*item.Quantity)) TotalPrice
  FROM [Test].[sales].[orders] Ord
  Inner join Sales.Customers cust on Ord.[customer_id]= cust.[customer_id]
  inner join [sales].[order_items] item on item.[order_id] = ord.[order_id]
  inner join [production].[products] prd on prd.product_id = item.product_id
group by [order_date],Cust.First_Name 
Order by Quantity desc</CommandText>
      </Query>
      <Fields>
        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderedYear">
          <DataField>OrderedYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalPrice">
          <DataField>TotalPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
