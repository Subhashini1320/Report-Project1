  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT TOP (1000) [product_id]
   ,Replace(Replace(Replace(Replace(REPLACE(REPLACE([product_name], '2016',''),'-',''),'2017',''),'2018',''),'2019',''),'2015/','') AS [product_name]
      ,[category_name]
	 ,cat.[category_id]
   FROM [Test].[production].[products] prd
  inner join [production].[categories] cat
  on cat.[category_id] = prd.[category_id]</CommandText>
      </Query>
      <Fields>
        <Field Name="product_id">
          <DataField>product_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="product_name">
          <DataField>product_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category_id">
          <DataField>category_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
