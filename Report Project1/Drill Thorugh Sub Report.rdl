  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProductID">
            <Value>=Parameters!ProductID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP (1000) [product_id]
      
     ,product_name
	 ,[category_name]
	 ,[Sub_category_name]
     ,[model_year]
	,[list_price]
  FROM [Test].[production].[products] prd
  inner join [production].[categories] cat
  on cat.[category_id] = prd.[category_id]
WHERE cat.[category_id] = @Category
AND  [product_id] = @ProductID</CommandText>
      </Query>
