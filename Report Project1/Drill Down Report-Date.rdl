  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	  Cust.First_Name as CustomerName 
	  ,([order_date] )
	  ,required_date
	  ,shipped_date
	  ,item.Quantity as Quantity
	  ,(item.List_price*item.Quantity) TotalPrice
	  ,prd.Product_name
	  ,str.Store_Name
  FROM [Test].[sales].[orders] Ord
  Inner join Sales.Customers cust on Ord.[customer_id]= cust.[customer_id]
  inner join [sales].[order_items] item on item.[order_id] = ord.[order_id]
  inner join [production].[products] prd on prd.product_id = item.product_id
  inner join [sales].[stores] str on str.store_id = ord.store_id
  WHERE Year([order_date]) =@Year
  and datename(month, [order_date])=@Month</CommandText>
      </Query>
