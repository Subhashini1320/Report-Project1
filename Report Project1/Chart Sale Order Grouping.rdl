    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FirstName">
            <Value>=Parameters!FirstName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT       Cust.customer_id, Cust.first_name, Cust.last_name, Ord.order_id, Ord.order_date, Ord.order_status, itm.quantity, itm.list_price, itm.discount
FROM            sales.customers AS Cust INNER JOIN
                         sales.orders AS Ord ON Ord.customer_id = Cust.customer_id INNER JOIN
                         sales.order_items AS itm ON itm.order_id = Ord.order_id
WHERE        (Cust.first_name = @FirstName)</CommandText>
      </Query>
